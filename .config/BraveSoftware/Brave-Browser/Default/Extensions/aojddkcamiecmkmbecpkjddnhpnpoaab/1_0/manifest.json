      "16": "images/logo-16.png",
      "24": "images/logo-24.png",
      "32": "images/logo-32.png",
      "48": "images/logo-48.png",
      "96": "images/logo-96.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "HD Camera Feed in Video Calls",
   "options_ui": {
      "chrome_style": true,
      "page": "settings.html"
   },
   "permissions": [ "https://*/*", "storage", "tabs" ],
